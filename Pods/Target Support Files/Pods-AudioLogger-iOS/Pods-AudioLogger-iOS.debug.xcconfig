ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/libopus/silk" $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/EZAudio" "${PODS_ROOT}/Headers/Public/TPCircularBuffer" "${PODS_ROOT}/Headers/Public/libogg" "${PODS_ROOT}/Headers/Public/libopus"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/EZAudio" "$PODS_CONFIGURATION_BUILD_DIR/TPCircularBuffer" "$PODS_CONFIGURATION_BUILD_DIR/libogg" "$PODS_CONFIGURATION_BUILD_DIR/libopus"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/EZAudio" -isystem "${PODS_ROOT}/Headers/Public/TPCircularBuffer" -isystem "${PODS_ROOT}/Headers/Public/libogg" -isystem "${PODS_ROOT}/Headers/Public/libopus"
OTHER_LDFLAGS = $(inherited) -ObjC -l"EZAudio" -l"TPCircularBuffer" -l"libogg" -l"libopus" -framework "AVFoundation" -framework "Accelerate" -framework "AudioToolbox" -framework "GLKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
